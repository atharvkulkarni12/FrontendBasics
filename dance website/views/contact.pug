extends base.pug

block scripts
  script(src='../static/script.js')

block scripts
  style
      include ../static/style.css
      include ../static/styleContact.css

block content 
    h1 Contact Us
    form(action="/contact", method="post", class="myForm")  
        input(type="text", class="myInput", name="name", placeholder="Enter your name")
        input(type="number", class="myInput", name="phone", placeholder="Enter your phone number")
        input(type="email", class="myInput", name="email", placeholder="Enter your valid email ID")
        input(type="number", class="myInput", name="age", placeholder="Enter your age")
        textarea(class="myInput", name="address", placeholder="Enter your address",rows="6" cols="33")
        textarea(class="myInput", name="concern", placeholder="Enter your concern",rows="6" cols="33")
        button.btn 
            | Submit